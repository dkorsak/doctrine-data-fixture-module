{
    "name": "dkorsak/doctrine-data-fixture-module",
    "description": "Zend Framework 3 Module that provides Doctrine Data-Fixture functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf3"
    ],
    "authors": [
        {
            "name": "Daniel Korsak",
            "email": "danielkorsak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "zendframework/zend-modulemanager": "~2.0",
        "zendframework/zend-eventmanager": "~3.0",
        "zendframework/zend-servicemanager": "~3.0",
        "doctrine/data-fixtures": "^1.1",
        "doctrine/doctrine-orm-module": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=3.7"
    },
    "autoload": {
        "psr-0": {
            "DoctrineDataFixtureModule": "src/"
        }
    }
}
